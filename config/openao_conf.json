{
	"pipeline": [
		{
			"uri": "openao:vonkarman_stream",
			"params": {
				"r0": "0.02",
				"L0": "4.0",
				"pitch": "0.025",
				"screen_size": "2048",
				"win_width": "10",
				"win_height": "10",
				"start_x": "0",
				"start_y": "1024",
				"step_x": "2",
				"step_y": "0",
			}
		},
		{
			"uri": "openao:delay",
			"params": {
				"s": "0",
				"ns": "200000000"
			}
		},
		{
			"uri": "openao:udp_sink",
			"params": {
				"ip": "127.0.0.1",
				"port": "64730"
			}
		},
		{
			"uri": "openao:file_sink",
			"params": {
				"filename": "data.oao"
			}
		},
		{
			"uri": "openao:delay",
			"params": {
				"s": "0",
				"ns": "800000000",
				"_comment": "hi"
			},
			"_comment": {
				"1": "this second delay is here to demonstrate",
				"2": "that (1) you can add comments via extra",
				"3": "json keys, and (2) that you can have",
				"4": "multiple copies of a device with",
				"5": "different parameters"
			}
		}
	]
}

